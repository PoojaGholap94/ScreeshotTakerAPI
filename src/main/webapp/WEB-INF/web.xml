<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
 
  <servlet>
  	<servlet-name>screenshotapi</servlet-name>
  	<display-name>screenshotapi</display-name>
  	<description>api</description>
  	<servlet-class>JavaAPI.rest.screenshotapi</servlet-class>
  </servlet>  
  

<servlet-mapping>
	<servlet-name>screenshotapi</servlet-name>
	<url-pattern>/screenshotapi</url-pattern>
</servlet-mapping>  
  
<welcome-file-list>  
    <welcome-file>index.html</welcome-file>  
</welcome-file-list>  
  
  
</web-app>
